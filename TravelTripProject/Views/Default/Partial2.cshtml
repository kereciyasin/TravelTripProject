@model List<TravelTripProject.Models.Blog>


<div class="right">
    @foreach (var item in Model)
    {
        <div class="bot-left">
            <img src="@item.ImageUrl" alt="" />
        </div>
        <div class="left-text">
            <h4>@item.Title</h4>
            <span class="doller">@item.Date</span>
            <p>@item.Description</p>
            <p>
                <a href="#"><span class="read">Read More >></span></a>
            </p>
        </div>
    }
</div>